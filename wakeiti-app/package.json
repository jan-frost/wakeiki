{
  "name": "com.janfrost.wakeiti",
  "displayName": "Wakeiti",
  "version": "1.0.0",
  "description": "A sample Apache Cordova application that responds to the deviceready event.",
  "main": "index.js",
  "scripts": {
    "build": "powershell -ExecutionPolicy Bypass -File build.ps1",
    "test": "elm-test",
    "build_android": "cordova build android",
    "build_browser": "cordova build browser",
    "start_android": "cordova run android",
    "start_browser": "cordova run browser",
    "prepare": "cd .. && husky install wakeiti-app/.husky"
  },
  "keywords": [
    "ecosystem:cordova"
  ],
  "author": "Apache Cordova Team",
  "license": "Apache-2.0",
  "devDependencies": {
    "cordova-android": "^13.0.0",
    "cordova-browser": "^7.0.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.10"
  },
  "cordova": {
    "platforms": [
      "android",
      "browser"
    ]
  },
  "lint-staged": {
    "*.elm": "elm-format --yes"
  }
}
